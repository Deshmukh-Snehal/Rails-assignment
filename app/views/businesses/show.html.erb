<div class="card">
  <div class="card-header">
    <p id="notice"><%= notice %></p>

    <p>
      <strong>Name:</strong>
      <%= @business.name %>
    </p>

    <p>
      <strong>Address:</strong>
      <%= @business.address %>
    </p>

    <p>
      <strong>Start Date:</strong>
      <%= @business.start_date.strftime("%Y-%m-%d at %I:%M %p") %>
    </p>

    <p>
      <strong>End Date:</strong>
      <%= @business.end_date.strftime("%Y-%m-%d at %I:%M %p") %>
    </p>

      <%= link_to 'Edit', edit_business_path, class:"btn btn-secondary" %> <br><br>
      <%= link_to 'Back', businesses_path, class:"btn btn-secondary"%>
   </div>
</div>