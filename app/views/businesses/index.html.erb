<div class="card">
  <div class="card-header">
    <h4>Business List</h4>
    <div class="card-body">
        <div class="form-group">
          <%= link_to "Export CSV", businesses_path(format: "csv"), class: 'btn btn-primary' %>
          <%= form_tag import_businesses_path, multipart: true, class: 'form-inline' do %>
        </div>
        <br/>
        <div class="form-group">
          <%= file_field_tag :file, class: '' %>
          <%= submit_tag "Import CSV", class: 'btn btn-info' %>
        </div>
        <% end %>
      <table class="table table-bordered table-striped" id="business-list"> 
      </table>    
    </div>
    <%= link_to 'New business', new_business_path, class: "btn btn-dark" %>
  </div>
</div>
<br>